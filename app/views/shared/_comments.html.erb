<% @comments.each do |comment| %>
  <div class="row comments">
    <div class= "span8">
      <div class="well">
        <p><%= comment.text %></p>
        Author: <%= link_to_user_page(comment.commenter) %>
        <%= time_ago_in_words(comment.created_at) %> ago
      </div>
    </div>
  </div>
<% end %>